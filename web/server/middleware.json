{
  "initial:before": {
    "loopback#favicon": {}
  },
  "initial": {
    "compression": {},
    "cors": {
      "params": {
        "origin": true,
        "credentials": true,
        "maxAge": 86400
      }
    }
  },
  "session": {},
  "auth": {},
  "parse": {},
  "routes": {
    "loopback#rest": {
      "paths": [
        "${restApiRoot}"
      ]
    }
  },
  "files": {
    "loopback#static": [{
      "params": "$!../dist/client",
      "paths": ["/"]
    }, {
      "params": "$!../dist/dashboard",
      "paths": ["/dashboard"]
    }]
  },
  "final": {
    "loopback#urlNotFound": {},
    "./middleware/handle404": {}
  },
  "final:after": {
    "loopback#errorHandler": {}
  }
}
