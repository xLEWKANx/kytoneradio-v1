.slide#cliclable(
  ng-repeat="poster in this.posters.postersArr"
  my-poster-place
  ng-mousedown="posters.writeCoords($event)"
  )
  a(
    nf-if="poster.local" ng-href="{{poster.outerUrl}}" target="_blank" rel="external"
    class="poster-link")
    img(ng-src="{{poster.pictureUrl}}" ng-click="posters.openPost($event, poster.outerIndex, poster.innerIndex, posters.startCoords)")




